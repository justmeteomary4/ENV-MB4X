EXECUTE OPEN 7 "O3_test.dat" UNKNOWN;
VARIABLE
O3
O1D
O3P
OH
HO2
H2O2
CH3O
CH3O2
CH3OOH
NO
NO2
O3f;
* O2, CO, CH4, H2O parameters are taken from ANsNEW;
PARAMETER
O2 5.0D18
CO 5.0D12
CO2 9.0D15
CH4 4.25D13
H2O 0!, 2.5D17
*N2 1.95D19 add when there will be 3 HO2+HO2 = H2O2 reactions
*M 2.55D19 may be include it later;
TEMP 288;
* NO, NO2 initial values are taken from ANsNEW;
COMPILE INITIAL;
*O3 6.25D+11 let's assume zero concentration;
OH 2.0D5
NO 2.0D12
NO2 1.0D11
**;
COMPILE EQUATIONS;
* Experiment without ANs (0 or control);
* O3 photolysis rate coef. is taken from ANsNEW (parameter)
% 2.0D-6                        : O3 = O1D;
% 2.2D-10                       : O1D+H2O = OH+OH; *plumea;
% 2.2D-13                       : OH+CO = HO2+CO2; *plumea;
% 2.4D-12*EXP(-1710./TEMP)      : OH+CH4 = CH3O2; *plumea;
% 2.2D-13*EXP(600./TEMP)        : HO2+HO2 = H2O2; *IUPACsummary=plumea;
% 7.7D-14*EXP(1300./TEMP)       : HO2+CH3O2 = CH3OOH; *plumea;
% 1.4D-14*EXP(-600./TEMP)       : HO2+O3 = OH; *plumea;
% 3.7D-12*EXP(240./TEMP)        : HO2+NO = NO2+OH; *plumea;
% 2.3D-12*EXP(360/TEMP)*0.999   : CH3O2+NO = NO2+CH3O; *MCM;
% 1.0D3                         : NO2 = NO+O3P; *ANsNEW;
% 1.8D-14                       : O3P+O2 = O3; *ANsNEW;
* Experiment with ANs (1);
*% 2.3D-12*EXP(360/TEMP)*0.001   : CH3O2+NO = CH3NO3; *MCM;
*% J<51>                         : CH3NO3 = CH3O+NO2; *MCM;
*% 4.0D-13*EXP(-845/TEMP)        : CH3NO3+OH = HCHO+NO2; *MCM;
*% J<3>                          : HCHO = HO2+HO2+CO;
*% J<4>                          : HCHO = CO+H2;
*% 1                             : HCHO+OH = HO2+CO;

= 1.8D-14                       : = O3f;
*= 5.8D-34*M*(TEMP/300.0)@-2.6   : = O3f;
**;
COMPILE TABLE;
PSTREAM 3;
**;
WHENEVER
TIME=0.0 0.5 1.0 1.5 2.0 2.5 3.0 3.5 4.0 4.5 5.0 5.5 6.0% CALL TABLE;
**;
SETPSTREAM 3 7;
TIME O3 ;
**;
BEGIN;
STOP;